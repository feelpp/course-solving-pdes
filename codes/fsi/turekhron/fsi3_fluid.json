// -*- mode: javascript -*-
{
    "Name": "Fluid fsi",
    "ShortName":"FluidFSI",
    "Model":"Navier-Stokes",
    "Parameters":
    {
        "ubar":"2",
        "chi":"t<2:t"
    },
    "Materials":
    {
        //"Fluid":{ "name":"Glycerine","filename":"$feelpp_srcdir/databases/materials/pure/copper.json" }
        "Fluid": {
            "name":"fluidFSI3",
            "rho":"1.0e3",
            "mu":"1.0"
        }
    },
    "BoundaryConditions":
    {
        "velocity":
        {
            "Dirichlet":
            {
                "fluid-inlet":
                {
                    "expr":"{ 1.5*ubar*(4./0.1681)*y*(0.41-y)*( ((1-cos(pi*t/2))/2)*chi + (1-chi) ),0}:ubar:y:t:chi"
                },
                "fluid-wall":
                {
                    "expr":"{0,0}"
                },
                "fluid-cylinder":
                {
                    "expr":"{0,0}"
                }
            },
            "interface_fsi":
            {
                "fsi-wall":
                {
                    "expr":"0"
                }
            }
        },
        "fluid":
        {
            "outlet":
            {
                "fluid-outlet":
                {
                    "expr":"0" 
                }
            }
        }
    },
    "PostProcess":
    {
        "Fields":["velocity","pressure","displacement","pid"],
        "Measures":
        {
            "Forces":["fsi-wall","fluid-cylinder"],
            "Points":
            {
                "pointB":
                {
                    "coord":"{0.15,0.2,0}",
                    "fields":"pressure"
                }
            }
        }
    }

}
